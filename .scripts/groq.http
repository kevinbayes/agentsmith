POST https://api.groq.com/openai/v1/chat/completions
Authorization: Bearer {{groq_token}}
Content-Type: application/json

{
"model" : "llama-3.1-8b-instant",
"stream" : false,
"messages" : [ {
  "role" : "system",
  "content" : "You are a helpful assistant to a financial banker who screens fraudulent individuals."
}, {
  "role" : "user",
  "content" : [ {
      "type" : "text",
      "text" : "Tell me what your job is?"
    } ]
} ],
"temperature" : 1.0,
"max_tokens" : 500,
"seed" : 0,
"top_p" : 1
}

