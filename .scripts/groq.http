POST https://api.groq.com/openai/v1/chat/completions
Authorization: Bearer {{groq_token}}
Content-Type: application/json

{
  "messages": [
    {
      "role": "user",
      "content": "Explain the importance of fast language models"
    }
  ],
  "model": "llama3-8b-8192"
}

